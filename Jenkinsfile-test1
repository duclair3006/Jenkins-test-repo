pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
             sh '''
             ls
             pwd
             touch eric
             '''
            }
        }

stage('Hello1') {
            steps {
             sh '''
             uname -r
             free
             '''
            }
        }

stage('Hello2') {
            steps {
             sh '''
              top
              nproc
             '''
            }
        }


stage('Hello3') {
            steps {
             sh '''
             ls -a
             ls -l
             mkdir qwerty
             '''
            }
        }







    }







}
